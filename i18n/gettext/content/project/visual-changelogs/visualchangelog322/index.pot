msgstr ""

#: content/project/visual-changelogs/visualchangelog322/index.md:247
msgid "Change the Z values of vertices"
msgstr ""

#: content/project/visual-changelogs/visualchangelog322/index.md:248
msgid "Move vertices"
msgstr ""

#: content/project/visual-changelogs/visualchangelog322/index.md:249
msgid "Interaction with edges"
msgstr ""

#: content/project/visual-changelogs/visualchangelog322/index.md:250
msgid "Split faces"
msgstr ""

